pipeline {
   agent {
     label 'KonIkkair'
   }

    environment {
        gitCredentialId='a94e0d0a-a27c-414b-8ee1-f502bf9311b3'
        gitUrl='https://github.com/ikkair/kairshop.git'
        deploymentBranch='master'
    }

    stages {
        stage('Cloning From GITHUB'){
            steps {
                script {
                    sh "echo Starting Clone From Gitlab " + gitUrl
                    git(
                        url:gitUrl,
                        gitCredentialsId:gitCredentialId,
                        branch:deploymentBranch
                    )
                    sh "Finish Clone From Gitlab" + gitUrl
                }
            }
        }
   }
 }
